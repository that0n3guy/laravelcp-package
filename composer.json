{
    "name": "gcphost/laravelcp",
    "description": "Laravel 4 Bootstrap Admin Site",
    "keywords": ["laravel","Starter Site", "Admin Site", "Portal"],
    "license": "MIT",
    "authors": [
        {
            "name": "William Bowman",
            "email": "gcphost@gmail.com"
        }
    ],
    "repositories": [
        {
            "url": "https://github.com/that0n3guy/confide.git",
            "type": "git"
        },
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.1.*",
        "zizaco/confide": "3.2.3b",
	"zizaco/entrust": "dev-master",
	"anlutro/l4-settings": "dev-master",
	"bllim/datatables": "*",
	"thomaswelton/laravel-gravatar": "dev-master",
	"khill/lavacharts" : "dev-master",
	"regulus/activity-log": "dev-master",
	"raahul/larryfour": "1.*",
	"hybridauth/hybridauth": "*",
	"msurguy/honeypot": "dev-master",
	"liebig/cron": "dev-master",
	"atticmedia/anvard": "dev-master",
	"gcphost/bootstrap3": "dev-master",
	"pda/pheanstalk": "2.0.*"

    },
    "autoload": {
        "classmap": [
		"src/commands",
		"src/controllers",
		"src/models",
		"src/database/migrations",
		"src/database/seeds",
		"src/services",
		"src/helpers",
		"src/repositories"
        ],
        "psr-0": {
            "Gcphost\\Laravelcp\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
